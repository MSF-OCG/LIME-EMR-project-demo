client

dev tun

remote ${{ secrets.OVPN_REMOTE_TCP }}
remote ${{ secrets.OVPN_REMOTE_UDP }}

data-ciphers-fallback AES-256-CBC
tls-cipher ${{ secrets.OVPN_TLS_CYPHER }}
auth SHA256

nobind
resolv-retry infinite
persist-key
persist-tun

remote-cert-tls server
auth-nocache

ca ca.crt
cert user.crt
key user.key

compress lz4

verb 1

auth-user-pass

auth-retry interact

reneg-sec 0
